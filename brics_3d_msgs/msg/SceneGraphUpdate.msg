# This message aggragates all possible update messages.
byte ADD_NODE=0 
byte ADD_GROUP=1
byte ADD_TRANSFORM_NODE=2
byte ADD_GEOMATIC_NODE=3
byte SET_NOTE_ATTRIBUTES=4
byte SET_TRANSFORM=5
byte DELETE_NOTE=6
byte ADD_PARENT=7

# Has one of the above values to specify the kind of update.
byte command

# id is used in the following commands: SET_NOTE_ATTRIBUTES, SET_TRANSFORM,
# DELETE_NOTE, ADD_PARENT to ifentify the node.
uint32 id

# parentId is used in the following commands: ADD_NODE, ADD_GROUP,
# ADD_TRANSFORM_NODE, ADD_GEOMATIC_NODE and ADD_PARENT to specify the parent node.
uint32 parentId

# assignedId is gnored by default. Only iff forcedId is set to true than this value 
# will be taken as input. It is relevant for the commands ADD_NODE, ADD_GROUP, 
# ADD_TRANSFORM_NODE and ADD_GEOMATIC_NODE. 
uint32 assignedId 

# Relavant for ADD_NODE, ADD_GROUP, ADD_TRANSFORM_NODE, ADD_GEOMATIC_NODE
# and SET_NOTE_ATTRIBUTES.
Attribute[] attributes

# Shape data is only used to the ADD_GEOMATIC_NODE command.
arm_navigation_msgs/Shape shape

# Trnasform data is relevant for ADD_TRANSFORM_NODE and SET_TRANSFORM.
geometry_msgs/TransformStamped transform

# Time stamps neet to be specified for all spactial data updates including
# ADD_TRANSFORM_NODE, ADD_GEOMATIC_NODE and SET_TRANSFORM-
time stamp

# If true then parentId will be assigned instead of an autogenerated one.
# Relevant vor ADD_NODE, ADD_GROUP, ADD_TRANSFORM_NODE and ADD_GEOMATIC_NODE. 
# Default is false.
bool forcedId  


